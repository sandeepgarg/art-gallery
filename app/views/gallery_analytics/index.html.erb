<div class="container">
  <table class="table table-striped table-bordered">
    <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>action</th>
    </tr>
    </thead>
    <tbody>
    <% @log_files.each do |log_file| %>
        <tr>
          <td>
            <%= log_file.id %>
          </td>
          <td>
            <%= log_file.name %>
          </td>
          <td>
            <%= link_to "Result", gallery_analytic_path(log_file.id) %>
          </td>
        </tr>
    <% end %>
    </tbody>
  </table>
  <div class="flickr_pagination">
    <%= will_paginate @log_files %>
  </div>
</div>


